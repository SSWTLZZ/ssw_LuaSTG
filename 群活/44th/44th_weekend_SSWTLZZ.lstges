4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"self.t = {}\r\nself.tex = tex\r\nfor i = 1, 4 do\r\n\tself.t[i] = {(i - 1) / 3 * 256, 0}\r\nend\r\nfor i = 5, 7 do\r\n\tself.t[i] = {256, (i - 4) / 3 * 256}\r\nend\r\nfor i = 8, 10 do\r\n\tself.t[i] = {256 - (i - 7) / 3 * 256, 256}\r\nend\r\nfor i = 11, 12 do\r\n\tself.t[i] = {0, 256 - (i - 10) / 3 * 256}\r\nend","EditWindow":"code"}],"AttributeCount":1}
