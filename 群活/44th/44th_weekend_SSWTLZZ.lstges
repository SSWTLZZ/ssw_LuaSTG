4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"self.point_list = {}\r\nself.point_list2 = {}\r\nlocal p = self.p\r\nlocal s = 0.5\r\nfor i = 1, #self.t do\r\n\tlocal v = (i) % #self.t + 1\r\n\tself.point_list[i] = {\r\n\t\t{(self.t[i][1] - 128) * s, (self.t[i][2] - 128) * s, 0.5, self.t[i][1], self.t[i][2], Color(255, 255, 255, 255)},\r\n\t\t{(self.t[v][1] - 128) * s, (self.t[v][2] - 128) * s, 0.5, self.t[v][1], self.t[v][2], Color(255, 255, 255, 255)},\r\n\t\t{(p[1] - 128) * s, (p[2] - 128) * s, 0.5, p[1], p[2], Color(255, 255, 255, 255)},\r\n\t\t{(p[1] - 128) * s, (p[2] - 128) * s, 0.5, p[1], p[2], Color(255, 255, 255, 255)},\r\n\t\tx = self.x, y = self.y, vx = ran:Float(-2, 2), vy = ran:Float(-1.5, 1.5), t = ran:Int(0, 360)\r\n\t}\r\n\tself.point_list2[i] = {\r\n\tx = self.x, y = self.y,\r\n\tvx = ran:Float(-2, 2) * 2, vy = ran:Float(1.5, 3) * 2, vy2 = ran:Float(0.5, 1)\r\n\t}\r\nend","EditWindow":"code"}],"AttributeCount":1,"IsExpanded":false}
